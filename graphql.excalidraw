    {
      "id": "qFIjsy389fAgCpep6PcwD",
      "type": "text",
      "x": 225,
      "y": 1612,
      "width": 1278.339111328125,
      "height": 425,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
      "roughness": 1,
      "opacity": 100,
      "groupIds": [],
